{
  "mcpServers": {
    "exa": {
      "url": "https://mcp.exa.ai/mcp"
    },
    "svelte": {
      "command": "npx",
      "args": ["-y", "@sveltejs/mcp"]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest"]
    },
    "mui-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@mui/mcp@latest"]
    },
    "Ref": {
      "type": "http",
      "_comment": "Replace YOUR_API_KEY with your Ref API key (ref.tools)",
      "url": "https://api.ref.tools/mcp?apiKey=REF_API_KEY_PLACEHOLDER"
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/tseitz/t-configs",
        "/Users/tseitz/code/projects",
        "/Users/tseitz/Library/CloudStorage/Dropbox/Tegan/Obsidian/tegan",
        "/Users/tseitz/Library/CloudStorage/Dropbox/TransferMusic"
      ]
    },
    "sqlite": {
      "command": "uvx",
      "args": [
        "mcp-server-sqlite",
        "--db-path",
        "/Users/tseitz/code/projects/evolutionary-speakers/data/evolutionary_speakers.db"
      ]
    }
  }
}